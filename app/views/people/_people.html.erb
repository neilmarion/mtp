
<tr id="table_header">
  <th><%= sort_link @q, :last_name, t('.last_name'), params, remote: true %></th>
  <th><%= sort_link @q, :first_name, t('.first_name'), params, remote: true %></th>
  <th><%= sort_link @q, :middle_name, t('.middle_name'), params, remote: true %></th>
  <th><%= t('.offices') %></th>
  <th><%= t('.organizations') %></th>
</tr>

<% people.each do |person| %>
  <%= render 'people/person', person: person %>
<% end %>
